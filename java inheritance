// tamim code
class Animal {
    
    public void eat() {
        System.out.println("This animal eats food.");
    }

    public void sleep() {
        System.out.println("This animal sleeps.");
    }
}


class Dog extends Animal {
    
    public void bark() {
        System.out.println("The dog barks.");
    }
}

public class InheritanceExample {
    public static void main(String[] args) {
       
        Dog dog = new Dog();

        
        dog.eat();   
        dog.sleep(); 
        dog.bark();  
    }
}
